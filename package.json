{
  "name": "jest-network-mock",
  "version": "0.0.2",
  "description": "Network api mock for jest",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "shx rm -rf ./dist && tsc -b ./tsconfig.prod.json",
    "test": "jest",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "lint:prettier": "prettier --check {**/*,*}.{js,ts,json,md}",
    "lint:eslint": "eslint {**/*,*}.{js,ts} --ignore-path ./.prettierignore"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoDocCat/jest-network-mock.git"
  },
  "keywords": [
    "jest",
    "jest-mock"
  ],
  "author": "NoDocCat <nodoccat@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NoDocCat/jest-network-mock/issues"
  },
  "homepage": "https://github.com/NoDocCat/jest-network-mock#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  }
}
