{
  "name": "jest-network-mock",
  "version": "1.2.0",
  "description": "Network api mock for jest",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "shx rm -rf ./dist && tsc -b ./tsconfig.prod.json",
    "test": "jest",
    "lint": "prettier -c -u . && eslint .",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "lint-staged": {
    "**/*": "prettier -w -u",
    "*.{js,jsx,ts,tsx}": [
      "prettier -w -u",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoDocCat/jest-network-mock.git"
  },
  "keywords": [
    "jest",
    "jest-mock"
  ],
  "author": "NoDocCat <nodoccat@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NoDocCat/jest-network-mock/issues"
  },
  "homepage": "https://github.com/NoDocCat/jest-network-mock#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
}
